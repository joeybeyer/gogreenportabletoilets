{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "cities:merge": "node scripts/mergeCitiesFromSheet.js",
    "cities:pages": "node scripts/generateCityPages.js",
    "cities:build": "npm run cities:merge && npm run cities:pages",
    "areas:pages": "node scripts/generateStateHubs.js",
    "build:site": "npm run cities:build && npm run areas:pages && npm run build && node scripts/buildSitemap.js",
    "sitemap:diff": "node scripts/reportSitemapDiff.js",
    "verify:cities": "node scripts/mergeCitiesFromSheet.js --dry && node scripts/generateCityPages.js --dry",
    "cities:prune": "PRUNE=true node scripts/generateCityPages.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.87.1",
    "csv-parse": "^6.1.0",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
